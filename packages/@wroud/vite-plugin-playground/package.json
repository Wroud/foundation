{
  "name": "@wroud/vite-plugin-playground",
  "type": "module",
  "version": "0.1.4",
  "sideEffects": [],
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "scripts": {
    "ci:release": "ci release --prefix vite-plugin-playground-v",
    "ci:git-tag": "ci git-tag --prefix vite-plugin-playground-v",
    "ci:release-github": "ci release-github --prefix vite-plugin-playground-v",
    "build": "tsc -b",
    "watch:tsc": "tsc -b -w",
    "clear": "rimraf lib",
    "test": "vitest run"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib",
    "src/**/*.svg",
    "src/**/*.css",
    "!lib/**/*.d.ts.map",
    "!lib/**/*.test.js",
    "!lib/**/*.test.d.ts",
    "!lib/**/*.test.d.ts.map",
    "!lib/**/*.test.js.map",
    "!lib/tests",
    "!.tsbuildinfo"
  ],
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@types/node": "^22",
    "@types/picomatch": "^3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@wroud/ci": "workspace:^",
    "@wroud/playground-react": "workspace:^",
    "@wroud/tsconfig": "workspace:*",
    "@wroud/vite-plugin-ssg": "workspace:^",
    "react": "^19",
    "react-dom": "^19",
    "rimraf": "^6",
    "vite": "^6.3.0",
    "vitest": "^2"
  },
  "peerDependencies": {
    "@wroud/playground-react": "workspace:^",
    "@wroud/vite-plugin-ssg": "workspace:^",
    "react": "^19",
    "react-dom": "^19"
  },
  "dependencies": {
    "@ariakit/react": "^0",
    "@tailwindcss/typography": "^0",
    "@vscode/codicons": "^0",
    "@wroud/navigation": "workspace:^",
    "@wroud/playground": "workspace:^",
    "@wroud/react-reactive-value": "workspace:^",
    "@wroud/react-split-view": "workspace:^",
    "@wroud/react-tree": "workspace:^",
    "front-matter": "^4",
    "magic-string": "^0",
    "picomatch": "^4",
    "react-markdown": "^10",
    "tailwindcss": "^4",
    "tinyglobby": "^0",
    "typescript": "^5"
  }
}
