{
  "name": "@wroud/playground-react",
  "type": "module",
  "sideEffects": [],
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "scripts": {
    "ci:release": "ci release --prefix playground-react-v",
    "ci:git-tag": "ci git-tag --prefix playground-react-v",
    "ci:release-github": "ci release-github --prefix playground-react-v",
    "build": "tsc -b",
    "watch:tsc": "tsc -b -w",
    "clear": "rimraf lib",
    "test": "vitest run"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib",
    "!lib/**/*.d.ts.map",
    "!lib/**/*.test.js",
    "!lib/**/*.test.d.ts",
    "!lib/**/*.test.d.ts.map",
    "!lib/**/*.test.js.map",
    "!lib/tests",
    "!.tsbuildinfo"
  ],
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@wroud/ci": "workspace:^",
    "@wroud/tsconfig": "workspace:^",
    "react": "^19",
    "react-dom": "^19",
    "rimraf": "^6",
    "typescript": "^5",
    "vitest": "^2"
  },
  "dependencies": {
    "@wroud/navigation": "workspace:^",
    "@wroud/playground": "workspace:^",
    "@wroud/vite-plugin-ssg": "workspace:^",
    "react-markdown": "^10"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  }
}
