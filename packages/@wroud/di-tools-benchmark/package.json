{
  "name": "@wroud/di-tools-benchmark",
  "type": "module",
  "private": true,
  "sideEffects": [],
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "scripts": {
    "build:legacy-decorators": "tsc --build tsconfig.legacy-decorators.json",
    "build:common": "tsc --build tsconfig.common.json",
    "build:modern": "tsc --build tsconfig.modern.json",
    "build:benchmark": "tsc --build tsconfig.json",
    "build": "yarn build:common && yarn build:modern && yarn build:legacy-decorators && yarn build:benchmark",
    "bundle": "yarn build && yarn node ./lib/benchmark/benchmark/bundle.js",
    "bench": "benchmark run",
    "clear": "rimraf lib"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib",
    "!lib/**/*.d.ts.map",
    "!lib/**/*.test.js",
    "!lib/**/*.test.d.ts",
    "!lib/**/*.test.d.ts.map",
    "!lib/**/*.test.js.map",
    "!lib/tests",
    "!.tsbuildinfo"
  ],
  "packageManager": "yarn@4.9.1",
  "devDependencies": {
    "@wroud/tests-runner": "workspace:^",
    "@wroud/tsconfig": "workspace:^",
    "esbuild": "^0",
    "rimraf": "^6",
    "typescript": "^5",
    "vitest": "^3"
  },
  "dependencies": {
    "@needle-di/core": "^1",
    "@wroud/di": "workspace:^",
    "@wroud/di-latest": "npm:@wroud/di@0.15.2",
    "brandi": "^5",
    "inversify": "^6",
    "inversify7": "npm:inversify@^7",
    "reflect-metadata": "^0",
    "tslib": "^2",
    "tsyringe": "^4"
  }
}
