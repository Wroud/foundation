{
  "name": "@wroud/di-tools-analyzer",
  "description": "DI Tools Analyzer is a tool that helps you to analyze the dependency injection tools in your project and generate a report for them.",
  "version": "0.0.3",
  "packageManager": "yarn@4.2.2",
  "type": "module",
  "license": "MIT",
  "author": "Wroud",
  "homepage": "https://wroud.dev/guide/package/di-tools/analyzer/introduction",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wroud/foundation",
    "directory": "packages/di-tools-analyzer"
  },
  "sideEffects": [],
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*",
    "./package.json": "./package.json",
    "./src": "./src/index.ts",
    "./src/*": "./src/*"
  },
  "files": [
    "LICENSE",
    "package.json",
    "README.md",
    "lib",
    "!lib/**/*.test.js",
    "!lib/**/*.test.d.ts",
    "!lib/**/*.test.d.ts.map",
    "!lib/**/*.test.js.map",
    "!.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b",
    "clear": "rimraf ./lib",
    "watch:tsc": "tsc -b -w",
    "dev": "concurrently \"npm:watch:*\""
  },
  "devDependencies": {
    "@types/d3": "^7",
    "@types/node": "^20",
    "@types/uuid": "^9",
    "@wroud/tsconfig": "workspace:^",
    "concurrently": "^8",
    "rimraf": "^6",
    "tslib": "^2",
    "typescript": "^5"
  },
  "keywords": [
    "dependency-injection",
    "di",
    "di-tools",
    "analyzer",
    "tools",
    "report"
  ],
  "dependencies": {
    "@wroud/di": "workspace:^",
    "d3": "^7",
    "uuid": "^10"
  }
}
